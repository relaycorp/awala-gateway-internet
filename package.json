{
  "name": "@relaycorp/relaynet-internet-gateway",
  "version": "1.0.0",
  "description": "Relaynet-Internet Gateway",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s static-checks test:unit",
    "test:unit": "jest --coverage",
    "test:functional": "jest --config jest.config.functional.js --runInBand",
    "static-checks": "run-p static-checks:*",
    "static-checks:lint": "tslint --project .",
    "static-checks:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:ci:unit": "run-s build test:ci:unit:jest cov:send",
    "test:ci:unit:jest": "jest --config jest.config.ci.js --coverage",
    "cov": "run-s build test:unit && opn coverage/lcov-report/index.html",
    "cov:send": "codecov",
    "clean": "trash build test"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@relaycorp/shared-config": "^1.3.3",
    "@types/jest": "^25.1.3",
    "@types/mongoose": "^5.7.14",
    "@types/pkijs": "0.0.2",
    "@types/verror": "^1.10.3",
    "codecov": "^3.5.0",
    "docker-compose": "^0.23.4",
    "dotenv": "^8.2.0",
    "fastify-plugin": "^1.6.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "npm-run-all": "^4.1.5",
    "pkijs": "^2.1.84",
    "prettier": "^1.19.1",
    "trash-cli": "^1.4.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.18.0",
    "typescript": "^3.8.3"
  },
  "prettier": "@relaycorp/shared-config/.prettierrc.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relaycorp/relaynet-internet-gateway.git"
  },
  "keywords": [
    "relaynet"
  ],
  "author": {
    "email": "no-reply@relaycorp.tech",
    "name": "Relaycorp, Inc.",
    "url": "https://relaycorp.tech/"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/relaycorp/relaynet-internet-gateway/issues"
  },
  "homepage": "https://docs.relaycorp.tech/relaynet-internet-gateway/",
  "dependencies": {
    "@relaycorp/cogrpc": "^1.1.5",
    "@relaycorp/keystore-vault": "^1.1.2",
    "@relaycorp/relaynet-core": "^1.28.2",
    "@relaycorp/relaynet-pohttp": "^1.5.0",
    "@typegoose/typegoose": "^6.2.2",
    "@types/pino": "^6.3.0",
    "aws-sdk": "^2.654.0",
    "axios": "^0.19.1",
    "buffer-to-arraybuffer": "0.0.5",
    "env-var": "^5.2.0",
    "fastify": "^2.11.0",
    "fastify-mongoose": "^0.3.0",
    "grpc": "^1.24.2",
    "it-pipe": "^1.1.0",
    "make-promises-safe": "^5.1.0",
    "mongoose": "^5.9.10",
    "node-nats-streaming": "^0.2.6",
    "pino": "^5.16.0",
    "stream-to-it": "^0.2.0",
    "uuid-random": "^1.3.0"
  }
}
